<link rel="stylesheet" href="/stylesheets/sweetalert.css">
<script type="text/javascript" src="/javascripts/sweetalert.min.js"></script>
<script src="/javascripts/socket.io.js" charset="utf-8"></script>
<script type="text/javascript">

  var socket = io();
  console.log(socket);
  var audio = new Audio('/fire.mp3');
  var audio2 = new Audio('/noti.mp3');
  socket.on('calling',function (data) {
    audio.play();
    swal({
    title: "Request Bill Payment",
    text: "Request from Table "+data,
    type: "success",
    confirmButtonText: "OK",
    closeOnConfirm: true
  },function (isConfirm) {
    audio.pause();

  });
});
socket.on('notiTo',function (data) {
  audio2.play();
  swal({
  title: "New Order arrived",
  text: "Order from Table "+data,
  type: "success",
  confirmButtonText: "OK",
  closeOnConfirm: true
},function (isConfirm) {
  audio2.pause();

});
});
</script>
