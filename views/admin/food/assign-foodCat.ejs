  </body>
  <script type="text/javascript">
    $(function(){
      $('#btnassign').click(function(){
        var data = {cat_name: $('#foodCat').val()};
        if(data.cat_name == ''){
          alert('Please add category!');
          return;
      }
        $.ajax({url: "/admin/duplicateCat", data:data, type:'POST', success: function(result){
          if(result.status == false){
            alert(result.msg);
            $('#foodCat').val('');
            $('#foodCat').focus();
          }else {
            frm.submit();
          }
        }});
      });
    });
  </script>
</html>
